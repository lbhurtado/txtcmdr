{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "bensampo/laravel-enum": "^1.16",
        "fideloper/proxy": "^4.0",
        "globelabs/globe-connect-php": "dev-master",
        "joselfonseca/laravel-tactician": "^0.5.0",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/tinker": "^1.0",
        "league/pipeline": "^1.0",
        "opis/events": "^5.0",
        "opis/string": "^1.5",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "propaganistas/laravel-phone": "^4.1",
        "spatie/laravel-model-status": "^1.6",
        "spatie/laravel-schemaless-attributes": "^1.3",
        "spatie/once": "^2.0",
        "spatie/string": "^2.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/App/Helpers/Utils.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
